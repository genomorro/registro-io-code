<form method="post" action="{{ path('app_visitor_delete', {'id': visitor.uuid}) }}" onsubmit="return confirm('{% trans %}Are you sure you want to delete this item?{% endtrans %}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ visitor.id) }}">
    <button class="btn btn-danger">{% trans %}Delete{% endtrans %}</button>
</form>
