{% extends 'base.html.twig' %}

{% block title %}Stakeholder{% endblock %}

{% block body %}
    <h1>Stakeholder</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ stakeholder.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ stakeholder.name }}</td>
            </tr>
            <tr>
                <th>Dni</th>
                <td>{{ stakeholder.dni }}</td>
            </tr>
            <tr>
                <th>Tag</th>
                <td>{{ stakeholder.tag }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ stakeholder.subject }}</td>
            </tr>
            <tr>
                <th>Destination</th>
                <td>{{ stakeholder.destination }}</td>
            </tr>
            <tr>
                <th>CheckInAt</th>
                <td>{{ stakeholder.checkInAt ? stakeholder.checkInAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CheckOutAt</th>
                <td>{{ stakeholder.checkOutAt ? stakeholder.checkOutAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Evidence</th>
                <td>{{ stakeholder.evidence }}</td>
            </tr>
            <tr>
                <th>Sign</th>
                <td>{{ stakeholder.sign }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_stakeholder_index') }}">back to list</a>

    <a href="{{ path('app_stakeholder_edit', {'id': stakeholder.id}) }}">edit</a>

    {{ include('stakeholder/_delete_form.html.twig') }}
{% endblock %}
